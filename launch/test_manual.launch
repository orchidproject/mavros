<launch>
  <node pkg="joy" type="joy_node" name="joy_node" output="screen" />
  <node pkg="mosaic_tester" type="flyByJoy_roscopter.py" name="flybyjoy" output="screen" />
  <node pkg="ar2mav" type="arproxy.py" name="arproxy" respawn="false" output="screen" args="-f $(find ar2mav)/map.csv -v 1 -l 192.168.1.3" />
  <node ns="apm" pkg="mavros" type="driver.py" name="mavros" output="screen" args="--device=udp:192.168.1.3:57812">
  </node>
  <node ns="mosaic" pkg="mavros" type="mosaic_node.py" name="mosaic_node" output="screen" />
</launch>
